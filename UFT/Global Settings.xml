<?xml version="1.0" encoding="utf-8"?>
<Environment>
	<!-- General Settings -->
	<Variable>
		<Name>ProjectName</Name>
		<Value>COmpasR2</Value>
	</Variable>
	<Variable>
		<Name>DataReferenceIdentifier</Name>
		<Value>#</Value>
	</Variable>
	<Variable>
		<Name>DefaultDataSheet</Name>
		<Value>General_Data</Value>
	</Variable>
	<!-- Execution Settings -->
	<Variable>
		<Name>RunConfiguration</Name>
		<Value>Regression</Value>
	</Variable>
	<Variable>
		<Name>OnError</Name>
		<Value>NextStep</Value>
	</Variable>
	<!-- Report Settings -->
	<Variable>
		<Name>IncludeTestDataInReport</Name>
		<Value>True</Value>
	</Variable>
	<Variable>
		<Name>TakeScreenshotFailedStep</Name>
		<Value>True</Value>
	</Variable>
	<Variable>
		<Name>TakeScreenshotPassedStep</Name>
		<Value>False</Value>
	</Variable>
		<Variable>
		<Name>WaitTime</Name>
		<Value>120000</Value>
	</Variable>
	<Variable>
		<Name>ReportsTheme</Name>
		<Value>MYSTIC</Value>
	</Variable>
	<Variable>
		<Name>HtmlReport</Name>
		<Value>True</Value>
	</Variable>
	<Variable>
		<Name>ExcelReport</Name>
		<Value>False</Value>
	</Variable>
	<!-- LogLevel ranges between 0 to 5, with 0 being minimal reporting and 5 being highly detailed reporting -->
	<Variable>
		<Name>LogLevel</Name>
		<Value>4</Value>
	</Variable>
	<!-- User-defined Settings -->
	<Variable>
		<Name>ApplicationPath</Name>
		<Value>LTQA</Value>
	</Variable>
</Environment>
